 html * {
	font-family: "Roboto","Helvetica Neue",Arial,sans-serif;
}

.topper {
  position: relative;
  padding: 10px 15px;
  background: #fefefe;
  color: #ffffff;
  margin-left: 194px;
  transition: all 0.5s ease;
}

#sidebar-scroll {
    position: fixed;
    width: 194px;
    margin-top:50px;
    overflow-y: scroll;
    top: 0;
    bottom: 0;
}

div.dt-buttons a.dt-button{
	background-color:red;
	background-image:none;
}

td.actions, th.actions {
    width: 1%;
    white-space: nowrap;
}

.chk-row {
    opacity: 100 !important;
}
/*login page*/
.flexship-logo{
    max-width: 50%;
    height: auto;
}
.rotating-text {
    background: rgba(17, 84, 170, 0.8);
    color: white;
    clear: both;
    overflow: invisible;
    margin: 5px 10px 5px 10px;
    padding: 20px;
    white-space: pre-wrap;
    display: inline-block;
}

.rotating-text-header {
    font-size: 300%;
    line-height: 50px;
}

.rotating-text-paragraph {
    font-size: 120%;
    line-height: 30px;
}

/*.content {
    position: absolute;
		bottom: 40px;
}*/

.right-panel {
    min-height: 100vh;
}

.left-panel {
    top: 50px;
    max-width: 100%;
    margin-left: 0;
    animation: bounceInDownSmall 1s ease;
    -webkit-animation: bounceInDownSmall 1s ease;
    -moz-animation: bounceInDownSmall 1s ease;
    -ms-animation: bounceInDownSmall 1s ease;
    -o-animation: bounceInDownSmall 1s ease;
}

.login {
    max-width: 70%;
    margin: auto;
}

img.rotating-image {
    height: 100vh;
    width: 100%;
}

.owl-controls{
  position: absolute !important;
  margin-left: auto !important;
  margin-right: auto !important;
  bottom: 10px !important;
  left: 0 !important;
  right: 0 !important;
}

.owl-theme .owl-controls .owl-page span {
  background: white !important;
  -webkit-box-shadow: inset 0px 13px 22px -5px rgba(0,0,0,0.65) !important;
  -moz-box-shadow: inset 0px 13px 22px -5px rgba(0,0,0,0.65) !important;
  box-shadow: inset 0px 13px 22px -5px rgba(0,0,0,0.65) !important;
}
.owl-theme .owl-controls .owl-page.active span {
    background-color: blue !important;
}


aside {
	background-image:url(<%= asset_path 'dashboard_menu_bg.jpeg' %>) !important;
	background-repeat:no-repeat;
	background-size:cover !important;
	text-transform: uppercase;
	padding-top: 50px !important;
}

aside * {
		font-size:12px;
}

aside .sidebar-overlay, .brand-overlay{
	background-color:#660000;
	position:absolute;
	opacity:0.5;
	height:100%;
	width:100%;
	z-index: -1;
}

aside *{
	border:0px !important;
}


td.actions, th.actions {
	width: 1%;
	white-space: nowrap;
}

.chk-row {
	opacity: 100 !important;
}

#top-nav, #top-nav .brand{
	background-color:transparent !important;
	min-height: 50px;
}

#main-search-bar{
	margin-left: 194px;
    height: 45px;
    background-color: white;
		min-height: 50px;
}

#main-search-bar input, #main-search-bar input:focus{
	border: solid 0px black !important;
  margin-top: 7px;
  height: 30px;
  width: 35%;
  margin-left: 5px;
  float: left;
}

.size-toggle{
	display:none;
}

#top-nav.skin-6 .nav-notification > li > a{
	color:#FFFFFF !important;
}

#top-nav.skin-6 .nav-notification > li.open > a{
	color:#fcfcfc !important;
}

.dataTables_info a{
	background-color:transparent;
	color:#660000;
	text-decoration:underline;
}

